layout {
    background-color "transparent"

    focus-ring {
        active-color   "#ffb3b5"
        inactive-color "#9f8c8c"
        urgent-color   "#ffb4ab"
    }

    border {
        active-color   "#ffb3b5"
        inactive-color "#9f8c8c"
        urgent-color   "#ffb4ab"
    }

    shadow {
        color "#00000070"
    }

    tab-indicator {
        active-color   "#ffb3b5"
        inactive-color "#9f8c8c"
        urgent-color   "#ffb4ab"
    }

    insert-hint {
        color "#ffb3b580"
    }
}